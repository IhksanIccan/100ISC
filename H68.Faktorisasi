package H68;

import java.util.Scanner;

public class H68 {
    
    public static void main(String[] args) {
        Scanner can = new Scanner(System.in);
        System.out.print("Masukkan bilangan : ");
        int a = can.nextInt();
        System.out.print("Faktorisasi Prima : "+ a);
        for (int i = 2; i <= a; i++){
            while(a % i == 0 ){
                System.out.print(i +" ");
                a /= i;
            }
        }
        System.out.println(" ");
    }
}
