package H88;

import java.util.Scanner;

public class H88 {
    
    public static void main(String[] args) {
        Scanner can = new Scanner(System.in);
        System.out.print("Masukkan nilai : ");
        int nilai = can.nextInt();
        evaluasiNilai(nilai);

        can.close();
    }
    public static void evaluasiNilai(int nilai) {
        if (nilai >= 80) {
            System.out.println("Nilai Kamu A");
        } else if (nilai >= 70) {
            System.out.println("Nilai Kamu B");
        } else if (nilai >= 60) {
            System.out.println("Nilai Kamu C");
        } else if (nilai >= 50) {
            System.out.println("Nilai Kamu D");
        } else {
            System.out.println("Nilai Kamu E");
        }
    }
}
