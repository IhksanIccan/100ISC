package H74;

public class H74 {
    
    public static void main(String[] args) {
        int[] angka = {1, 2, 3, 4, 5,37,76,47,98,43};
        
        for (int i = 0; i < angka.length; i++) {
            if (angka[i] % 2 == 0) {
                System.out.println(angka[i] + " adalah bilangan genap");
            } else {
                System.out.println(angka[i] + " adalah bilangan ganjil");
            
            }
        }
    }
}
