package latihanarray;
public class LatihanArray {
    public static void main(String[] args) {
        int a[] = {234,235,10,-14,-12,567,458,125,67,24,78,189};
        int jumlaindex = a.length;
        int max = a[0],min = a[0];
        System.out.print("Deret Bilangan : ");
        for (int i = 0; i < jumlaindex; i++) {
            System.out.print(a[i]+" ");
            if (a[i] > max){
                max = a[i];
            }
            if (a[i] < min){
                min = a[i];
            }
        }
        System.out.println("");
        System.out.println("MAX : "+max);
        System.out.println("MIN : "+min);
    }
    
}
